{
  "name": "puppetter-typescript-test-template",
  "version": "1.0.0",
  "scripts": {
    "test": "jest"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "@types/puppeteer": "^1.12.4",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^4.0.0",
    "puppeteer": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.+(ts|tsx|js)"
    ]
  }
}
